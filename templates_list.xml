	<module>
		<name>
			CT Head Checklist
		</name>
		<content>
			<multi_radio>
				<radio label="Acute infact" negativetext="There is no acute cortical infarct.">
					Not present | Present
				</radio>
				<radio label="Acute haemorrhage" negativetext="There is no acute intracranial haemorrhage.">
					Not present | Present
				</radio>
				<radio label="Collection" negativetext="No intracranial collection is present.">
					Not present | Present
				</radio>
				<radio label="Space occupying lesion" negativetext="There are no space occupying lesions.">
					Not present | Present
				</radio>
			</multi_radio>
			<multi_select label="White matter changes" askfurtherdetails="true">
				<select label="Severity" print_text_before="There is periventricular and deep white matter hypodensity which is non-specific but may represent a" print_text_after="burden of small vessel disease">
					mild | moderate | severe
				</select>
			</multi_select>
			<multi_select label="Cerebral atrophy" askfurtherdetails="true">
				<select label="Severity" print_text_before="There is">
					mild | moderate | severe
				</select>
				<select label="Location" print_text_after="cerebral atrophy">
					supratentorial | infratentorial | generalised
				</select>
			</multi_select>
			<multi_select label="Hippocampal atrophy" askfurtherdetails="true">
				<select label="Location" print_text_before="There is">
					bilateral | right | left
				</select>
				<select label="Severity" print_text_after="hippocampal atrophy">
					mild | moderate | severe
				</select>
			</multi_select>
			<selection label="Bones">
				There are no skull fractures or destructive bony lesions
			</selection>
			<multi_radio>
				<radio label="Venous sinus hyperdensity" negativetext="The venous sinuses are not hyperdense.">
					Normal | Abnormal
				</radio>
				<radio label="Basilar hyperdensity">
					Not present | Present
				</radio>
				<radio label="Trauma/contusions">
					Not present | Present
				</radio>
				<radio label="Orbits">
					Normal | Abnormal
				</radio>
				<radio label="Old infarcts">
					Not present | Present
				</radio>
				<radio label="Paranasal sinuses and mastoid air cells">
					Normal | Abnormal
				</radio>
				<radio label="Clivus">
					Normal | Abnormal
				</radio>
				<radio label="Pituitary">
					Normal | Abnormal
				</radio>
				<radio label="Cerebellar tonsils">
					Normal | Abnormal
				</radio>
				<radio label="Extracranial soft tissue">
					Normal | Abnormal
				</radio>
			</multi_radio>
		</content>
